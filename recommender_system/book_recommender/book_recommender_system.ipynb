   "source": [
    "# books with more than 250 rating (to implement popularity)\n",
    "popular_books = final_rating[final_rating['Num-Rating']>250].sort_values(by='Avg-Rating', ascending=False).reset_index(drop=True).head(50)\n",
    "print(popular_books.head(5))"
   ]
