   "source": [
    "books   = pd.read_csv('dataset/Books.csv')[['ISBN', 'Book-Title']]\n",
    "ratings = pd.read_csv('dataset/Ratings.csv')\n",
    "users   = pd.read_csv('dataset/Users.csv')\n",
    "print(books.head())"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### Popularity based filtering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
